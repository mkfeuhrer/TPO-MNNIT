            android:label="@string/title_activity_tpo_home"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".student.profile.MyProfile" />
        <activity android:name=".student.profile.SectionsPageAdapter" />
        <activity android:name=".student.profile.Tab1Fragment" />
        <activity android:name=".student.profile.Tab2Fragment" />
        <activity android:name=".student.profile.Tab3Fragment" />
        <activity android:name=".tpo.AddCompany" />
        <activity
            android:name=".student.company.CompanyStudent"
            android:label="@string/title_activity_company_student"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".student.InterviewExperience.interviewexperience" />
        <activity android:name=".student.InterviewExperience.AddInterviewExperience" />
        <activity android:name=".tpo.VerifyUser" />
        <activity android:name=".tpo.StudentFilter" />
        <activity android:name=".tpo.MessageTpo" />
        <activity android:name=".tpo.ManageStudents" />
